{
  "id": "/rt-estonia-vehicles-service",
  "instances": 1,
  "cpus": 0.5,
  "mem": 256,
  "disk": 0,
  "gpus": 0,
  "fetch": [
    {
      "uri": "http://172.16.1.4/docker.tar.gz",
      "extract": true,
      "executable": false,
      "cache": false
    }
  ],
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "peatusee.azurecr.io/rt-estonia-vehicles-service:testing",
      "network": "BRIDGE",
      "privileged": false,
      "forcePullImage": true,
      "portMappings": [
        {
          "containerPort": 0,
          "hostPort": 10004,
          "protocol": "tcp",
          "name": "default"
        }
      ]
    }
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "killSelection": "YOUNGEST_FIRST",
  "requirePorts": true,
  "env": {
    "OTPURL": "http://opentripplanner-estonia:8080/otp/routers/estonia/index/graphql",
    "MQTTCLIENTURL": "mqtt://mosquitto-server:1883",
    "MQTTCLIENTUSER": "publisher",
    "MQTTCLIENTPASS": "sHalLnoTpaSS"
  }
}