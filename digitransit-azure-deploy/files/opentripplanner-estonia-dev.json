{
    "uris":  [
        "https://loovsys.eu/docker.tar.gz"
    ],
  "acceptedResourceRoles": null,
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": null,
  "constraints": [],
  "container": {
      "docker": {
          "forcePullImage": true,
          "image": "peatusee.azurecr.io/opentripplanner",
          "network": "BRIDGE",
          "parameters": [
              {
                  "key": "net",
                  "value": "host"
              },
              {
                  "key": "name",
                  "value": "opentripplanner-estonia"
              }
          ],
          "portMappings": [
              {
                  "containerPort": 8080,
                  "hostPort": 0,
                  "labels": {
                      "VIP_0": "/opentripplanner-estonia:8080"
                  },
                  "name": "otpestonia",
                  "protocol": "tcp",
                  "servicePort": 10007
              }
          ],
          "privileged": false
      },
      "type": "DOCKER",
      "volumes": []
  },
  "cpus": 1,
  "dependencies": [],
  "disk": 0,
  "env": {
      "ROUTER_DATA_CONTAINER_URL": "http://api.dev.peatus.ee/routing-data/v2/estonia",
      "ROUTER_NAME": "estonia",
      "JAVA_OPTS": "-Xmx7G -Xms7G -XX:+UseG1GC -XX:MaxGCPauseMillis=200 -XX:G1ReservePercent=10"
  },
  "gpus": 0,
  "healthChecks": [
      {
          "command": {
              "value": "test $(curl -4 -s -o /dev/null -w \"%{http_code}\" http://localhost:$PORT0/otp/routers/estonia/) = 200"
          },
          "gracePeriodSeconds": 2400,
          "ignoreHttp1xx": false,
          "intervalSeconds": 30,
          "maxConsecutiveFailures": 6,
          "protocol": "COMMAND",
          "timeoutSeconds": 20
      }
  ],
  "id": "/opentripplanner-estonia",
  "instances": 1,
  "labels": {
      "restart-after-services": "/opentripplanner-data-con-estonia",
      "restart-delay": "25",
      "update": "auto"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 10240,
  "portDefinitions": [
      {
          "labels": {},
          "port": 10007,
          "protocol": "tcp"
      }
  ],
  "ports": [
      10007
  ],
  "readinessChecks": [],
  "taskKillGracePeriodSeconds": null,
  "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 0
  }
}
