{
  "id": "/mosquitto-server",
  "instances": 1,
  "cpus": 1,
  "mem": 2048,
  "disk": 0,
  "gpus": 0,
  "fetch": [
    {
      "uri": "http://172.16.1.4/docker.tar.gz",
      "extract": true,
      "executable": false,
      "cache": false
    }
  ],
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "peatusee.azurecr.io/mosquitto-server:prod",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 1883,
          "hostPort": 0,
          "servicePort": 10000,
          "protocol": "tcp",
          "name": "default",
          "labels": {
            "VIP_0": "/mosquitto-server:1883"
          }
        },
        {
          "containerPort": 9001,
          "hostPort": 0,
          "servicePort": 10001,
          "protocol": "tcp",
          "name": "websocket",
          "labels": {
            "VIP_1": "/mosquitto-server:9001"
          }
        }
      ],
      "privileged": false,
      "forcePullImage": true
    }
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "killSelection": "YOUNGEST_FIRST",
  "requirePorts": true,
  "env": {
    "MQTTCLIENTPASS": "sHalLnoTpaSS",
    "MQTTCLIENTUSER": "publisher"
  }
}