{
  "uris":  [
      "http://172.16.1.4/docker.tar.gz"
  ],
    "acceptedResourceRoles": null,
    "backoffFactor": 1.15,
    "backoffSeconds": 1,
    "cmd": null,
    "constraints": [
        [
             "hostname",
             "UNIQUE"
        ]
    ],
    "container": {
        "docker": {
            "forcePullImage": true,
            "image": "peatusee.azurecr.io/digitransit-ui",
            "network": "BRIDGE",
            "parameters": [
                {
                    "key": "net",
                    "value": "host"
                }
            ],
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0,
                    "labels": {
                        "VIP_0": "/digitransit-ui-default:8080"
                    },
                    "protocol": "tcp",
                    "servicePort": 10017
                }
            ],
            "privileged": false
        },
        "type": "DOCKER",
        "volumes": []
    },
    "cpus": 0.2,
    "dependencies": [],
    "disk": 0,
    "env": {
        "API_URL": "https://api.dev.peatus.ee",
        "ASSET_URL": "https://api.dev.peatus.ee/ui/v1/estonia",
        "CONFIG": "estonia",
        "MAP_URL": "https://api.dev.peatus.ee",
        "OTP_URL": "https://api.dev.peatus.ee/routing/v1/routers/estonia/",
        "GEOCODING_BASE_URL": "https://api.dev.peatus.ee/geocoding/v1",
        "NODE_OPTS": "--max_old_space_size=600",
        "PIWIK_ADDRESS": "",
        "PIWIK_ID": "",
        "RELAY_FETCH_TIMEOUT": "3000",
        "SENTRY_DSN": "https://sentryuser@sentry-analytics.dev.peatus.ee/peatusee",
        "SENTRY_SECRET_DSN": "https://sentryuser:sentrypassword@sentry-analytics.dev.peatus.ee/peatusee"
    },
    "gpus": 0,
    "healthChecks": [
        {
            "gracePeriodSeconds": 300,
            "ignoreHttp1xx": false,
            "intervalSeconds": 10,
            "maxConsecutiveFailures": 10,
            "path": "/",
            "portIndex": 0,
            "protocol": "HTTP",
            "timeoutSeconds": 10
        }
    ],
    "id": "/digitransit-ui-default",
    "instances": 1,
    "labels": {
        "restart-at": "03:00",
        "restart-limit-interval": "180",
        "update": "auto"
    },
    "maxLaunchDelaySeconds": 3600,
    "mem": 1024,
    "portDefinitions": [
        {
            "labels": {},
            "port": 10017,
            "protocol": "tcp"
        }
    ],
    "ports": [
        10017
    ],
    "readinessChecks": [],
    "taskKillGracePeriodSeconds": null,
    "upgradeStrategy": {
        "maximumOverCapacity": 1,
        "minimumHealthCapacity": 0
    }
}
